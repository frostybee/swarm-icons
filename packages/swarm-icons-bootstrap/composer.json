{
    "name": "frostybee/swarm-icons-bootstrap",
    "description": "Bootstrap Icons (~2,000 SVGs) for frostybee/swarm-icons",
    "type": "swarm-icons-set",
    "license": "MIT",
    "keywords": ["icons", "svg", "bootstrap", "swarm-icons"],
    "homepage": "https://github.com/frostybee/swarm-icons",
    "require": {
        "php": "^8.2",
        "frostybee/swarm-icons": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Frostybee\\SwarmIcons\\Bootstrap\\": "src/"
        }
    },
    "extra": {
        "swarm-icons": {
            "prefix": "bi",
            "provider-class": "Frostybee\\SwarmIcons\\Bootstrap\\BootstrapIconSet"
        }
    }
}
